{
  "name": "funds_management",
  "version": "1.0.0",
  "description": "Managing payment accounts like credit cards, banks etc",
  "main": "../dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node ../dist/index.js",
    "dev": "nodemon --watch src --exec \"node --inspect=9229 -r ts-node/register src/index.ts\"",
    "dev:client": "cd ../client && npm run dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:client\" --names \"backend,frontend\" --prefix-colors \"blue,magenta\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Almogyo/funds_management.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/Almogyo/funds_management/issues"
  },
  "homepage": "https://github.com/Almogyo/funds_management#readme",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^12.4.6",
    "bull": "^4.15.4",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "fuzzball": "^2.2.3",
    "helmet": "^8.1.0",
    "israeli-bank-scrapers": "^6.3.1",
    "jsonwebtoken": "^9.0.2",
    "redis": "^4.7.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.18.3",
    "zod": "^4.1.13"
  },
  "optionalDependencies": {
    "python-Levenshtein": "^0.20.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.1",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "prettier": "^3.7.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
